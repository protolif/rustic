<h1>New Computer</h1>

<%= form_for(@computer) do |f| %>
  <%= render 'shared/error_messages', :object => f.object %>
  <div class='field'>
    <%= f.label :make %>
    <%= f.text_field :make %>
  </div>
  <div class='field'>
    <%= f.label :model %>
    <%= f.text_field :model %>
  </div>
  <div class='field'>
    <%= f.label :serial %>
    <%= f.text_field :serial %>
  </div>
  <%= hidden_field_tag :user_id, params[:user_id] %>
  <div class='actions'>
    <%= f.label nil %>
    <%= f.submit "Submit" %>
  </div>
<% end%>