<fieldset>
	<legend>Ticket</legend>
	<% if !t.computer.checked_in.nil? %>
	  <b>Checked in:</b>
	  <%= "#{t.computer.checked_in.strftime('%B %d %I:%M %p')}
	      (#{checked_in_at(t.computer)})" %><br />
  <% end %>
  <% if !t.computer.checked_out.nil? %>
    <b>Checked out:</b>
	  <%= "#{t.computer.checked_out.strftime('%B %d %I:%M %p')}
	      (#{checked_out_at(t.computer)})" %><br />
  <% end %>
  <b>Status:</b> <%= t.status %><br />
  <b>Technician:</b>
  <%= (t.technician.nil?) ? 'None' : t.technician.fname %><br />
</fieldset>